<%= title t(:edit_resource, resource: t(:user, scope: 'activerecord.models')) %>

<%= content_for :right do %>
  <%= link_to t(:show), admin_user_path(@user) %>
  <%= link_to admin_users_path do %>
    <i class="fas fa-arrow-left"></i> <%= t(:back) %>
  <% end %>
<% end %>

<%= render 'form', user: @user %>

